// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Contact {
  id       Int     @id @default(autoincrement())
  name     String
  lastName String?
  phone    String  @unique
  avatar   String?
}
